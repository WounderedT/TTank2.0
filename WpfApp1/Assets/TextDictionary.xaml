<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ProfilerViewer.Assets">

    <FontFamily x:Key="CommonFont">Copperplate Gothic Light</FontFamily>

    <Style x:Key="CommonLabelStyle" TargetType="{x:Type Label}">
        <Setter Property="FontFamily" Value="{StaticResource CommonFont}"/>
        <Setter Property="FontSize" Value="15"/>
    </Style>
    
</ResourceDictionary>